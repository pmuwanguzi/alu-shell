#!/usr/bin/env bash
# This script groups visitors by IP and HTTP status code and displays the data

# Specify the path to your Apache access log file
log_file="apache-access.log"

# Use awk to process the log file, group by IP and HTTP status code, and sort the results
awk '{ print $1 " " $9}' "$log_file" | sort | uniq -c | sort -rn

